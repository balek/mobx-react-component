{
    "name": "mobx-react-component",
    "version": "0.52.0",
    "description": "Write React functional components (with hooks) + MobX for local state in a fancy way",
    "main": "dist/index.js",
    "jsnext:main": "dist/index.module.js",
    "module": "dist/index.module.js",
    "react-native": "dist/native.js",
    "typings": "dist/index.d.ts",
    "types": "dist/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/xaviergonz/mobx-react-component.git"
    },
    "scripts": {
        "prettier": "prettier --write \"**/*.js\" \"**/*.ts\"",
        "lint": "tslint --project .",
        "validate": "tsc --noEmit",
        "test": "jest --watch",
        "test:travis": "yarn validate && yarn lint && yarn coverage",
        "coverage": "jest --coverage",
        "prebuild": "rimraf dist && yarn copydocs",
        "build": "node build-rollup.js",
        "copydocs": "shx cp ../../*.md ."
    },
    "author": "Javier González Garcés",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/xaviergonz/mobx-react-component/issues"
    },
    "peerDependencies": {
        "mobx": "^4.0.0 || ^5.0.0",
        "react": "^16.8.0",
        "react-dom": "^16.8.6"
    },
    "devDependencies": {
        "@types/hoist-non-react-statics": "^3.3.1",
        "@types/jest": "^24.0.11",
        "@types/node": "^12.0.0",
        "@types/react": "^16.8.10",
        "@types/react-dom": "^16.8.3",
        "coveralls": "^3.0.3",
        "hoist-non-react-statics": "^3.3.0",
        "husky": "^2.1.0",
        "jest": "^24.5.0",
        "jest-mock-console": "^0.4.2",
        "mobx": "^5.9.4",
        "prettier": "^1.16.4",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-testing-library": "^7.0.0",
        "rimraf": "^2.6.3",
        "rollup": "^1.7.4",
        "rollup-plugin-alias": "^1.5.1",
        "rollup-plugin-commonjs": "^9.2.1",
        "rollup-plugin-node-resolve": "^4.0.1",
        "rollup-plugin-replace": "^2.1.1",
        "rollup-plugin-terser": "^4.0.4",
        "rollup-plugin-typescript2": "^0.21.0",
        "shx": "^0.3.2",
        "ts-jest": "^24.0.1",
        "tslint": "^5.14.0",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^3.4.1"
    },
    "keywords": [
        "mobx",
        "observable",
        "react-component",
        "react",
        "reactjs",
        "reactive",
        "hooks",
        "observer",
        "mobxObserver"
    ],
    "husky": {
        "hooks": {
            "pre-commit": "yarn validate && yarn lint & yarn copydocs"
        }
    },
    "dependencies": {},
    "files": [
        "package.json",
        "dist",
        "README.md",
        "CHANGELOG.md",
        "LICENSE"
    ]
}
